<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Data.AppConfiguration" Version="1.6.0" />
    <PackageReference Include="Azure.Identity" Version="1.11.4" />
    <PackageReference Include="YamlDotNet" Version="13.7.1" />
    <PackageReference Include="Riok.Mapperly" Version="4.2.0" PrivateAssets="all" />
  </ItemGroup>
  <ItemGroup>
    <!-- Make NBGV-provided commit properties available to code -->
    <CompilerVisibleProperty Include="GitCommitId" />
    <CompilerVisibleProperty Include="GitCommitIdShort" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../AppConfigCli.Core/AppConfigCli.Core.csproj" />
  </ItemGroup>
</Project>
